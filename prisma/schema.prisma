generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Chat {
  id       Int    @id @default(autoincrement())
  username String
  text     String

  createdAt DateTime @default(now()) @map("created_at")

  @@map("chat")
}
